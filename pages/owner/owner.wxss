.main {
  color: #aeb0b3;
  font-size: 30rpx;
  height: 900rpx;
  width: 100%;
  background-color: #fafafa;
  box-sizing: border-box;
  padding-top: 25rpx;
  position: relative;
  overflow: hidden;
  display: flex;
}

.left_sroll {
  width: 90%;
  /* height: 100%;  */
  background-color: #f5f5f5;
}

.left_sroll view {
  text-align: center;
  margin-top: -52rpx;
  height: 110rpx;
  line-height: 110rpx;
  margin-bottom: 14rpx;
  position: relative;
}

.left_sroll .alive {
  font-size: 32rpx;
  color: #313233;
  background: #ffffff;
}

.left_sroll .alive::before {
  content: "";
  display: block;
  height: 100%;
  width: 6rpx;
  background: rgb(235, 194, 14);
  padding-top: 10rpx;
  position: absolute;
}

