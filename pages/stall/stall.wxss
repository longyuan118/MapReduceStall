swiper {
  width: 100%;
  height: calc(100vw * 352 / 1125);
  /* height: 34.28vw; */
}

image {
  width: 100%;
}

map {
  width: 100%;
  height: 500px;
}

.add_icon {
  position: fixed;
  width: 42px;
  height: 42px;
  bottom: 30px;
  right: 20px;
}

.index_cate {
  display: flex;
}
.index_cate view {
  padding: 20rpx;
  flex: 1;
}
.index_cate view image {
  width: 80%;
}

.floor_title{
  float: left;
}
.floor_title image{
  width: 250rpx;
  height: 490rpx;
  border-radius: 10px;
}

.floor_index{
  float: right;
  margin-left: 5px;
}
.floor_index image{
  width: 120px;
  border-radius: 5px;
}



